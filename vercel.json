{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "x-powered-by", "value": "Next.js on Vercel" }]
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": ".*vercel\\.app" }],
      "headers": [{ "key": "x-robots-tag", "value": "noindex" }]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/sublet-calculator",
      "permanent": true,
      "has": [
        { "type": "host", "value": "zakaryayev.com|www.zakaryayev.com" }
      ]
    }
  ]
}
